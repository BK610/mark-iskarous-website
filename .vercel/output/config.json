{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_server-islands\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/_image\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/research\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/sanity\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/teaching\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}